{
  "name": "imagecharup",
  "dep":[
    "PHP4",
    "PHP5",
    "PHP7"
  ],
  "fun": " imagecharup(resource $image,int $font,int $x,int $y,string $c,int $color ):bool",
  "detail" :"垂直绘制一个字符",
  "param": [
	  {
		  "name": "$image",
		  "type": "resource",
		  "detail" : "绘画的背景图像",
		  "must": true
	  },
	  {
		  "name": "$font",
		  "type": "int",
		  "detail" : "字体类型 1到5为内置字体",
		  "must": true
	  },
	  {
		  "name": "$x",
		  "type": "int",
		  "detail" : "左上角x坐标",
		  "must": true
	  },
	  {
		  "name": "$y",
		  "type": "int",
		  "detail" : "左上角y坐标",
		  "must": true
	  },
	  {
		  "name": "$c",
		  "type": "string",
		  "detail" : "待绘画的字符",
		  "must": true
	  },
	  {
		  "name": "$color",
		  "type": "int",
		  "detail" : "使用的颜色",
		  "must": true
	  }
