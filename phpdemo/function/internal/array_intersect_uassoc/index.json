{
  "name": "array_intersect_uassoc",
  "dep":[
    "PHP5",
    "PHP7"
  ],
  "fun": " array array_intersect_uassoc(array $array1,array $array2 [,array $...],callable $key_compare_func )",
  "detail" :"使用自定义函数比较数组的值与键，获取交集",
  "param": [
    {
      "name": "$array1",
      "type": "array",
      "detail" : "待比较的第一个数组",
      "must": true
    },
    {
      "name": "$array2",
      "type": "array",
      "detail" : "待比较的第二个数组",
      "must": true
    },
    {
      "name": "$...",
      "type": "array",
      "detail" : "待比较的其他数组",
      "must": false
    },
    {
      "name": "$key_compare_func",
      "type": "callable",
      "detail" : "自定义的比较函数",
      "must": true
    }
  ],
  "return": [
    {
      "name": "$return",
      "type": "array",
      "detail": "比较后的交集数组"
    }
  ],
  "info": [
    "使用自定义函数比较数组的键名与值"
  ]
}