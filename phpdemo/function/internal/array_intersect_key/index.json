{
  "name": "array_intersect_key",
  "dep":[
    "PHP5 >= 5.1.0",
    "PHP7"
  ],
  "fun": "array array_intersect_key(array $array1, array $array2 [,$...] )",
  "detail" :"根据比较数组键名,获取数组的的交集",
  "param": [
    {
      "name": "$array1",
      "type": "array",
      "detail" : "待比较的第一个数组",
      "must": true
    },
    {
      "name": "$array2",
      "type": "array",
      "detail" : "待比较的第二个数组",
      "must": true
    },
    {
      "name": "$...",
      "type": "array",
      "detail" : "其他待比较的数组",
      "must": false
    }
  ],
  "return": [
    {
      "name": "$return",
      "type": "array",
      "detail": "返回交集数组"
    }
  ],
  "info": [
    "比较键名，获取同时存在的键名的交集数组"
  ]
}